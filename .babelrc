{
    "presets": [
        "es2015",
        "stage-0",
        "react"
    ],
    "plugins": [
        ["module-alias", [
            { "src": "./styles", "expose": "styles" },
            { "src": "./components", "expose": "project-components" },
            { "src": "./public/img/icons", "expose": "icons" }
        ]],
        "add-module-exports",
        "transform-runtime",
        "transform-flow-strip-types"
    ]
}