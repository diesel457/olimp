.AboutBox
	display flex
	flex-direction column
	align-items flex-end
	padding 85px 72px 75px

	&-inner
		width 50%

	&-items
		display flex
		justify-content space-between
		margin-top 55px

	&-item
		display flex
		flex-direction column
		align-items center
		&:first-child
			margin-left 32px
		& > span
			margin-top 50px
